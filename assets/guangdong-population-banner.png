<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="1200" height="300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0f9ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#d6eaff;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="title-gradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#005aa7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1b75bc;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="300" fill="url(#bg-gradient)" rx="10" ry="10" />
  
  <!-- Border with province outline -->
  <rect x="10" y="10" width="1180" height="280" fill="none" stroke="#1b75bc" stroke-width="3" rx="8" ry="8" />
  
  <!-- Title text -->
  <text x="600" y="80" font-family="Arial, sans-serif" font-size="42" font-weight="bold" text-anchor="middle" fill="url(#title-gradient)">Guangdong Province Population Flow Analysis</text>
  
  <!-- Subtitle text -->
  <text x="600" y="120" font-family="Arial, sans-serif" font-size="22" text-anchor="middle" fill="#444">广东省人口流动分析</text>
  
  <!-- Data visualization elements -->
  <!-- Flow lines -->
  <path d="M300,200 C400,160 500,230 600,190" stroke="#1b75bc" stroke-width="5" fill="none" opacity="0.6" />
  <path d="M400,180 C500,220 600,170 700,210" stroke="#1b75bc" stroke-width="4" fill="none" opacity="0.5" />
  <path d="M500,210 C600,160 700,230 800,180" stroke="#1b75bc" stroke-width="6" fill="none" opacity="0.7" />
  <path d="M600,190 C700,230 800,180 900,220" stroke="#1b75bc" stroke-width="3" fill="none" opacity="0.4" />
  
  <!-- City dots -->
  <circle cx="300" cy="200" r="12" fill="#e84118" opacity="0.8" />
  <circle cx="600" cy="190" r="14" fill="#e84118" opacity="0.8" />
  <circle cx="800" cy="180" r="10" fill="#e84118" opacity="0.8" />
  <circle cx="900" cy="220" r="8" fill="#e84118" opacity="0.8" />
  
  <!-- City names -->
  <text x="300" y="230" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#333">Guangzhou</text>
  <text x="600" y="220" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#333">Shenzhen</text>
  <text x="800" y="210" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#333">Dongguan</text>
  <text x="900" y="250" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#333">Foshan</text>
  
  <!-- Data statistics -->
  <rect x="80" y="170" width="150" height="80" fill="white" opacity="0.7" rx="5" ry="5" />
  <text x="155" y="200" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#333">Population Data</text>
  <text x="155" y="225" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#555">2008-2022</text>
  
  <rect x="970" y="170" width="150" height="80" fill="white" opacity="0.7" rx="5" ry="5" />
  <text x="1045" y="200" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#333">21 Cities</text>
  <text x="1045" y="225" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#555">Migration Analysis</text>
</svg>